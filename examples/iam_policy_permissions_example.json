{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "s3Allow",
      "Effect": "Allow",
      "Action": [
        "s3:*"
      ],
      "Resource": [
        "arn:aws:s3:::clumio-bulk-restore-dahye"
      ]
    },
    {
      "Sid": "lambdaAllow",
      "Effect": "Allow",
      "Action": [
        "states:*",
        "lambda:*"
      ],
      "Resource": [
        "*"
      ]
    },
    {
      "Sid": "logsAllow",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "*"
    }
  ]
}
